# GPU Worker 환경변수

# R2 Storage (S3 compatible)
STORAGE_S3_ENDPOINT=
STORAGE_ACCESS_KEY=
STORAGE_SECRET_KEY=
STORAGE_BUCKET=s3-images

# Processing
BATCH_CONCURRENCY=4
MODEL_CACHE_DIR=/models

# Callback
CALLBACK_TIMEOUT_SEC=10
GPU_CALLBACK_SECRET=

# Adapter (runpod | queue_pull)
ADAPTER=runpod

# Logging
LOG_LEVEL=info
